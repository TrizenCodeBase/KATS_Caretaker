{
  "expo": {
    "name": "KATS",
    "slug": "kats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./kats_logo.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "splash": {
      "image": "./kats_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kats.caretaker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./kats_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kats.caretaker"
    },
    "web": {
      "favicon": "./kats_logo.png"
    }
  }
}
